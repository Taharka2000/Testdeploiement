{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/health",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/contacts",
        "dest": "/api/contacts/index.js"
      },
      {
        "src": "/api/contacts/(.*)",
        "dest": "/api/contacts/[id].js"
      }
    ],
    "env": {
      "MONGO_URI": "@MONGO_URI"
    }
  }